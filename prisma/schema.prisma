generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas           = ["public", "auth"]
}

model Profile {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  email     String
  @@schema("public")
}
